{
  "manifestVersion": 1,
  "id": "ansible",
  "name": "Ansible",
  "version": "1.0.0",
  "publisher": "ms-devops-extensions",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Ansible extension for Azure DevOps",
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "images/icon.png"
  },
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "contributions": [
    {
      "id": "ansible-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Ansible"
      }
    }
  ],
  "files": [
    {
      "path": "Tasks"
    },
    {
      "path": "images",
      "addressable": true
    }
  ]
}
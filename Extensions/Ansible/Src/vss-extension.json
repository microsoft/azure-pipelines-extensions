{
  "manifestVersion": 1.0,
  "id": "vss-services-ansible",
  "name": "Ansible",
  "publisher": "ms-vscs-rm",
  "version": "0.0.7",
  "public": true,
  "description": "This extension executes an Ansible playbook using a specified inventory via command line interface",
  "_description.comment": "The below format to define extensions is currently in preview and may change in future.",
  "categories": ["Azure Pipelines"],
  "Tags": ["Ansible", "Release", "DevOps"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [],
  "icons": {
    "default": "images\\icon_default.png",
    "large": "images\\icon_wide.png"
  },
  "branding": {
    "color": "#3B3E43",
    "theme": "dark"
  },
  "screenshots": [
    {
      "path": "images\\ansible_screen_1.png"
    },
    {
      "path": "images\\ansible_screen_2.png"
    }
  ],
  "files": [
    {
      "path": "images\\ansible_screen_1.png",
      "addressable": true
    },
    {
      "path": "images\\ansible_screen_2.png",
      "addressable": true
    },
    {
      "path": "images\\ansible_endpoint.png",
      "addressable": true
    },
    {
      "path": "images\\icon_wide.png",
      "addressable": true
    },
    {
      "path": "Tasks/Ansible"
    }
  ],
  "content": {
    "details": {
      "path": "readme.md"
    }
  },
  "contributions": [
    {
      "id": "ansible-task",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "Tasks/Ansible"
      }
    }
  ]
}

      "id": "Microsoft.VisualStudio.Services.Cloud"
    },
    {
      "id": "Microsoft.TeamFoundation.Server"
    }
  ],
  "demands": [],
  "icons": {
    "default": "images/copyvar.png"
  },
  "files": [
    {
      "path": "Tasks/CopyAgentlessJobVariableToReleaseVariable"
